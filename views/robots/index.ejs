<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
	<div align="left">
	<a href="robots/new" class="btn btn-primary btn-sm" ><span class="glyphicon glyphicon-plus"></span></span>New Robot</a>
    </div>
	<ul>
      <% robots.forEach(function(robot){ %>
        <li><%= robot._id %> - <%= robot.name %> - <%= robot.description %></li>
      <% }); %>
    </ul>

  </body>
</html>